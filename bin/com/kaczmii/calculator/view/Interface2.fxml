<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="borderPane" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kaczmii.calculator.view.InterfaceController2">
   <center>
      <GridPane fx:id="gridPane" gridLinesVisible="true" prefHeight="330.0" prefWidth="300.0" BorderPane.alignment="CENTER">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="sqrt" mnemonicParsing="false" onAction="#ButtonsComplexAction" prefHeight="55.0" prefWidth="75.0" snapToPixel="false" text="√" GridPane.columnIndex="1">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="percent" alignment="CENTER" mnemonicParsing="false" onAction="#ButtonsComplexAction" prefHeight="55.0" prefWidth="75.0" snapToPixel="false" text="\%">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="square" mnemonicParsing="false" onAction="#ButtonsComplexAction" prefHeight="55.0" prefWidth="75.0" text="x²" GridPane.columnIndex="2">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="cube" mnemonicParsing="false" onAction="#ButtonsComplexAction" prefHeight="55.0" prefWidth="75.0" text="x³" GridPane.columnIndex="3">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="ce" mnemonicParsing="false" onAction="#ButtonsComplexAction" prefHeight="55.0" prefWidth="75.0" text="CE" GridPane.rowIndex="1">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="clear" mnemonicParsing="false" onAction="#ButtonsComplexAction" prefHeight="55.0" prefWidth="75.0" text="C" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="back" mnemonicParsing="false" onAction="#ButtonsComplexAction" prefHeight="55.0" prefWidth="75.0" text="&lt;-" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="divided" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="/" GridPane.columnIndex="3" GridPane.rowIndex="1">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="one" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="1" GridPane.rowIndex="2">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="two" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="2" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="three" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="3" GridPane.columnIndex="2" GridPane.rowIndex="2">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="times" mnemonicParsing="false" onAction="#ButtonsComplexAction" prefHeight="55.0" prefWidth="75.0" text="x" GridPane.columnIndex="3" GridPane.rowIndex="2">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="four" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="4" GridPane.rowIndex="3">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="five" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="5" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="six" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="6" GridPane.columnIndex="2" GridPane.rowIndex="3">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="minus" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="-" GridPane.columnIndex="3" GridPane.rowIndex="3">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="seven" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="7" GridPane.rowIndex="4">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="eight" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="8" GridPane.columnIndex="1" GridPane.rowIndex="4">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="nine" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="9" GridPane.columnIndex="2" GridPane.rowIndex="4">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="plus" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="+" GridPane.columnIndex="3" GridPane.rowIndex="4">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="plusminus" mnemonicParsing="false" onAction="#ButtonsComplexAction" prefHeight="55.0" prefWidth="75.0" text="+/-" GridPane.rowIndex="5">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="zero" defaultButton="true" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="0" GridPane.columnIndex="1" GridPane.rowIndex="5">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="coma" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="," GridPane.columnIndex="2" GridPane.rowIndex="5">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="equal" mnemonicParsing="false" onAction="#ButtonsTextToDIsplay" prefHeight="55.0" prefWidth="75.0" text="=" GridPane.columnIndex="3" GridPane.rowIndex="5">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
         </children>
      </GridPane>
   </center>
   <top>
      <TextField fx:id="textField" alignment="CENTER_RIGHT" centerShape="false" editable="false" prefHeight="70.0" prefWidth="300.0" BorderPane.alignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </TextField>
   </top>
</BorderPane>
